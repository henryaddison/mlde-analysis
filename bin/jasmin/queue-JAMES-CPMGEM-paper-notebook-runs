#!/bin/bash
# Script for queueing all the notebooks used for CPMGEM_pr paper

set -euo pipefail

SCRIPT_DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )

output_base_dirpath="${SCRIPT_DIR}/../../../mlde-notebooks"


for nb in distribution ai-schematic structure uq eval-at-60km-pretty; do #samples
  echo $nb
  bin/jasmin/queue-run-notebook nbs/evaluation/${nb}.ipynb ${output_base_dirpath} nbs/evaluation/parameters/paper.yml;
done

# CCS

for nb in ccs-annual ccs-seasonal ccs-threshold; do #samples
  echo $nb
  bin/jasmin/queue-run-notebook nbs/evaluation/${nb}.ipynb ${output_base_dirpath} nbs/evaluation/parameters/paper.yml;
done

for nb in ccs-seasonal-domain-mean-bootstrap ccs-seasonal-frac-contrib-bootstraps; do #samples
  echo $nb
  bin/jasmin/queue-run-notebook -m 512G nbs/evaluation/${nb}.ipynb ${output_base_dirpath} nbs/evaluation/parameters/paper.yml;
done

# lo data figures

for nb in lo-data-figures; do
  echo $nb
  bin/jasmin/queue-run-notebook nbs/evaluation/${nb}.ipynb ${output_base_dirpath} nbs/evaluation/parameters/paper-lo-data.yml;
done
